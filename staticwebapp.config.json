{
	"routes": [
		{
			"route": "/api/*",
			"allowedRoles": ["anonymous"]
		}
	],
	"navigationFallback": {
		"rewrite": "/index.html"
	},
	"mimeTypes": {
		".json": "application/json",
		".css": "text/css",
		".js": "application/javascript"
	},
	"globalHeaders": {
		"content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https:"
	}
}
