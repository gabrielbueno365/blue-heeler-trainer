{
	"routes": [
		{
			"route": "/api/*",
			"allowedRoles": ["anonymous"]
		}
	],
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/api/*", "/*.{css,js,png,jpg,jpeg,gif,svg,ico,json}"]
	},
	"responseOverrides": {
		"404": {
			"rewrite": "/index.html",
			"statusCode": 404
		}
	},
	"platform": {
		"apiRuntime": "python:3.10"
	},
	"globalHeaders": {
		"Cache-Control": "public, max-age=3600"
	},
	"mimeTypes": {
		".json": "application/json",
		".css": "text/css",
		".js": "application/javascript"
	}
}
